<Window x:Class="WpfApp2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dataGrid" HorizontalAlignment="Left" Height="374" Margin="10,35,0,0" VerticalAlignment="Top" Width="772" AutoGenerateColumns="True"/>
        <Button x:Name="searchByNumber" Content="Поиск по номеру" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="110" Click="searchByNumber_Click"/>
        <Button x:Name="uploadCSV" Content="Выгрузить CSV" HorizontalAlignment="Left" Margin="125,10,0,0" VerticalAlignment="Top" Width="100" Click="uploadCSV_Click"/>
        <Button x:Name="StreetsButton" Content="Улицы" HorizontalAlignment="Left" Margin="230,10,0,0" VerticalAlignment="Top" Width="75" Click="StreetsButton_Click"/>
        <Button x:Name="description" Content="Описание" HorizontalAlignment="Left" Margin="310,10,0,0" VerticalAlignment="Top" Width="75" Click="description_Click"/>

    </Grid>
</Window>
